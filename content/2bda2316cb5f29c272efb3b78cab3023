function validForm(bulkcable) { 
	mynewstring = parseInt(bulkcable.qty.value)
	if(bulkcable.qty.value=="") { 
		alert("You must indicate how many feet you wish to purchase before submitting.") 
		return false
		bulkcable.qty.value.focus() 
	} 
	else if(isNaN(mynewstring)) { 
		alert("Please use numbers only (ie. 47).") 
		return false
		bulkcable.qty.value.focus() 
	}
	else
	{
		return true
	}
}
