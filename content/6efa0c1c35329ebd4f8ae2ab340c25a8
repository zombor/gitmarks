			function getArgs() {
				var args = {};
				var pairs = location.search.substring(1).split("&");
				for(var i=0;i<pairs.length;i++) {
					var pos = pairs[i].indexOf("=");
					if(pos==-1) {continue;}
					args[pairs[i].substring(0,pos)] = unescape(pairs[i].substring(pos+1));
				}
				return args;
			}

			var dlabs = {
